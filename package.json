{
  "name": "gymrat-drizzle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3800",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc --pretty --project ./tsconfig.json --noEmit",
    "db:generate": "dotenv -e .env drizzle-kit generate:sqlite",
    "db:push": "dotenv -e .env drizzle-kit push:sqlite --verbose --strict",
    "db:check": "dotenv -e .env drizzle-kit check:sqlite"
  },
  "dependencies": {
    "@headlessui-float/react": "^0.13.1",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@libsql/client": "^0.4.1",
    "@shopify/react-form": "^2.5.5",
    "@tailwindcss/forms": "^0.5.7",
    "bcryptjs": "^2.4.3",
    "cookie-es": "^1.0.0",
    "date-fns": "^3.3.1",
    "downshift": "^8.3.1",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^11.0.3",
    "fuse.js": "^7.0.0",
    "iron-session": "^8.0.1",
    "next": "^14.1.0",
    "nodemailer": "^6.9.8",
    "pretty-cache-header": "^1.0.0",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.2.1",
    "usehooks-ts": "^2.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "better-sqlite3": "^9.3.0",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "prettier": {
    "tailwindConfig": "tailwind.config.ts",
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
      "<BUILT_IN_MODULES>",
      "",
      "<THIRD_PARTY_MODULES>",
      "",
      "@trip/(.*)$",
      "",
      "^~/(.*)$",
      "",
      "^[./]"
    ]
  }
}
