{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "tsc": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint", "tsc"],
      "outputs": []
    }
  }
}
